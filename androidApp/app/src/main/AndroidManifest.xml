<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.seeStore">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/SeeStore"
        tools:targetApi="31">
        <activity
            android:name=".activity.CartActivity"
            android:exported="false"
            android:label="@string/title_activity_cart"
            android:theme="@style/SeeStore.NoActionBar" />
        <activity
            android:name="com.example.seeStore.activity.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.seeStore.activity.SplashActivity"
            android:exported="true"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.SeeStore.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>